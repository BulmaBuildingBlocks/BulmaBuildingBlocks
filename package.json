{
  "name": "bulmabuildingblocks",
  "version": "0.5.5",
  "description": "A website to provide the ability to generate websites using html building blocks",
  "main": "./src/with-variables.scss",
  "homepage": "https://github.com/JosephAnson/BulmaBuildingBlocks",
  "bugs": {
    "url": "https://github.com/JosephAnson/BulmaBuildingBlocks/issues"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/JosephAnson/BulmaBuildingBlocks.git"
  },
  "engines": {
    "node": "10.16.1"
  },
  "author": "Joseph Anson",
  "keywords": [
    "bulma",
    "vue",
    "vuejs",
    "vue-bulma",
    "components",
    "html",
    "html-snippets"
  ],
  "peerDependencies": {
    "vue": "^2.5.18"
  },
  "scripts": {
    "build": "nuxt-ts build",
    "build:scss": "node-sass --importer node_modules/node-sass-package-importer/dist/cli.js --output-style expanded src/with-variables.scss | node ./build/banner.js > dist/bulmabuildingblocks.css && cleancss -o dist/bulmabuildingblocks.min.css dist/bulmabuildingblocks.css",
    "ci": "concurrently \"yarn run lint\" \"yarn run test:coverage -- --runInBand\"",
    "clean": "rimraf ./coverage ./shared/**/*.js .nuxt",
    "dev": "nuxt-ts",
    "e2e": "cypress open --browser chrome",
    "e2e:headless": "cypress run",
    "format": "concurrently \"yarn run format:scripts\" \"yarn run format:scss\"",
    "format:scripts": "prettier --check **/*.{vue,ts,scss,json} --ignore-path .gitignore --write",
    "format:scss": "npx stylelint ./docs/assets/styles/**/*.scss ./src/scss/**/*.scss --fix",
    "generate:images": "cypress run --config screenshotsFolder=docs/assets/component-images/,integrationFolder=cypress/generate --spec \"/**/*\"",
    "lint": "eslint --ext .vue,.ts --ignore-path .gitignore . --fix",
    "lint-watch": "esw --ext .vue,.ts --ignore-path .gitignore . --fix --watch",
    "postci": "codecov",
    "precommit": "concurrently \"yarn run lint\"",
    "predev": "yarn run clean",
    "release:major": "npm run build:scss && changelog -M && git add CHANGELOG.md && git commit -m \"chore: update changelog\" && npm version major",
    "release:minor": "npm run build:scss && changelog -m && git add CHANGELOG.md && git commit -m \"chore: update changelog\" && npm version minor",
    "release:patch": "npm run build:scss && changelog -p && git add CHANGELOG.md && git commit -m \"chore: update changelog\" && npm version patch",
    "start": "nuxt-ts start",
    "test": "cross-env NODE_ENV=test yarn run unit && yarn run e2e:headless",
    "unit": "cross-env NODE_ENV=test jest",
    "unit:coverage": "cross-env NODE_ENV=test jest --coverage",
    "unit:update": "cross-env NODE_ENV=test jest --u",
    "unit:watch": "jest --watchAll"
  },
  "husky": {
    "hooks": {
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS",
      "pre-commit": "yarn run precommit"
    }
  },
  "resolutions": {
    "prosemirror-model": "1.9.1",
    "prosemirror-state": "1.3.3",
    "prosemirror-transform": "1.2.4",
    "prosemirror-view": "1.14.6"
  },
  "dependencies": {
    "@fortawesome/fontawesome-free": "^5.12.1",
    "@nuxt/typescript-runtime": "^0.3.9",
    "@nuxtjs/dotenv": "^1.4.1",
    "@nuxtjs/pwa": "^3.0.0-beta.20",
    "@popperjs/core": "^2.2.1",
    "buefy": "^0.8.15",
    "bulma": "^0.8.2",
    "copy-to-clipboard": "^3.3.1",
    "core-js": "2.6.11",
    "file-saver": "^2.0.2",
    "jquery": "^3.4.1",
    "jszip": "^3.3.0",
    "jszip-utils": "^0.1.0",
    "nuxt": "^2.12.2",
    "raw-loader": "^4.0.0",
    "redirect-ssl": "^1.4.1",
    "require-context": "^1.1.0",
    "tiptap": "^1.26.6",
    "tiptap-extensions": "^1.28.6",
    "vue": "^2.6.11",
    "vue-highlightjs": "^1.3.3",
    "vue-progressbar": "^0.7.5",
    "vue-template-compiler": "^2.6.11",
    "vuedraggable": "^2.23.2"
  },
  "devDependencies": {
    "@babel/core": "^7.8.7",
    "@babel/plugin-proposal-class-properties": "^7.8.3",
    "@babel/plugin-proposal-decorators": "^7.8.3",
    "@commitlint/cli": "^8.3.5",
    "@commitlint/config-conventional": "^8.3.4",
    "@nuxt/typescript-build": "^0.6.2",
    "@nuxtjs/eslint-config": "^2.0.0",
    "@nuxtjs/eslint-module": "^1.1.0",
    "@nuxtjs/module-test-utils": "^1.5.0",
    "@nuxtjs/stylelint-module": "^3.2.1",
    "@types/glob": "^7.1.1",
    "@types/jest": "^25.1.1",
    "@types/jquery": "^3.3.33",
    "@types/lodash": "^4.14.149",
    "@types/node": "^13.7.0",
    "@types/popper.js": "^1.11.0",
    "@types/prettier": "^1.19.0",
    "@types/prosemirror-model": "^1.7.2",
    "@types/prosemirror-state": "^1.2.3",
    "@types/vue": "^2.0.0",
    "@typescript-eslint/eslint-plugin": "^2.18.0",
    "@typescript-eslint/parser": "^2.18.0",
    "@vue/server-test-utils": "^1.0.2",
    "@vue/test-utils": "^1.0.2",
    "babel-core": "^7.0.0-bridge.0",
    "babel-eslint": "^10.0.3",
    "babel-jest": "^25.1.0",
    "clean-css-cli": "^4.3.0",
    "codecov": "3.6.5",
    "concurrently": "3.6.0",
    "cross-env": "^7.0.0",
    "cypress": "^3.0.2",
    "eslint": "^6.8.0",
    "eslint-config-prettier": "^6.10.0",
    "eslint-plugin-nuxt": "^0.5.0",
    "eslint-plugin-prettier": "^3.1.2",
    "eslint-watch": "^6.0.1",
    "generate-changelog": "^1.8.0",
    "glob": "7.1.2",
    "html-loader": "^0.5.5",
    "husky": "^4.2.1",
    "jest": "^25.1.0",
    "jest-serializer-vue": "^2.0.2",
    "jest-transform-stub": "^2.0.0",
    "lint-staged": "^10.0.6",
    "lodash": "^4.17.15",
    "node-sass": "^4.13.1",
    "node-sass-package-importer": "^5.3.2",
    "nuxt-property-decorator": "^2.7.2",
    "prettier": "^1.19.1",
    "prosemirror-model": "^1.9.1",
    "prosemirror-state": "^1.3.3",
    "sass-loader": "^8.0.2",
    "stylelint": "^13.0.0",
    "stylelint-config-recommended": "^3.0.0",
    "stylelint-scss": "^3.14.2",
    "ts-jest": "^25.2.1",
    "ts-loader": "^6.2.2",
    "typescript": "^3.8.3",
    "vue-eslint-parser": "^7.0.0",
    "vue-jest": "^3.0.5",
    "vue-property-decorator": "^8.4.2",
    "vuex-module-decorators": "^0.16.1"
  }
}
