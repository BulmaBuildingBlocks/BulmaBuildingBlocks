{
  "name": "bulmabuildingblocks",
  "version": "0.5.6",
  "description": "Bulma Building Blocks allows you to generate websites using snippets based on the Bulma CSS Frameworks.",
  "main": "./src/with-variables.scss",
  "homepage": "https://github.com/BulmaBuildingBlocks/BulmaBuildingBlocks",
  "bugs": {
    "url": "https://github.com/BulmaBuildingBlocks/BulmaBuildingBlocks/issues"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/BulmaBuildingBlocks/BulmaBuildingBlocks.git"
  },
  "engines": {
    "node": "12.16.3"
  },
  "author": "Joseph Anson <me@josephanson.com>",
  "maintainers": [
    "Joseph Anson <me@BulmaBuildingBlocks.com>"
  ],
  "license": "MIT",
  "keywords": [
    "bulma",
    "vue",
    "vuejs",
    "vue-bulma",
    "components",
    "html",
    "html-snippets",
    "page-builder"
  ],
  "peerDependencies": {
    "vue": "^2.6.11"
  },
  "scripts": {
    "build": "nuxt-ts build",
    "build:scss": "node-sass --importer node_modules/node-sass-package-importer/dist/cli.js --output-style expanded src/with-variables.scss | node ./build/banner.js > dist/bulmabuildingblocks.css && cleancss -o dist/bulmabuildingblocks.min.css dist/bulmabuildingblocks.css && git add dist/bulmabuildingblocks.css && git add dist/bulmabuildingblocks.min.css && git commit -m \"chore: update dist files\"",
    "ci": "concurrently \"yarn run lint\" \"yarn run unit -- --runInBand\"",
    "clean": "rimraf ./coverage .nuxt",
    "dev": "nuxt-ts",
    "e2e": "percy exec -- cypress open --browser chrome",
    "e2e:headless": "cypress run",
    "e2e:headless:visual": "percy exec -- cypress run",
    "format": "concurrently \"yarn run format:scripts\" \"yarn run format:scss\"",
    "format:scripts": "prettier --check **/*.{vue,ts,scss,json} --ignore-path .gitignore --write",
    "format:scss": "npx stylelint ./docs/assets/styles/**/*.scss ./src/scss/**/*.scss --fix",
    "generate:images": "cypress run --config screenshotsFolder=docs/assets/component-images/,integrationFolder=cypress/generate,pageLoadTimeout=600000 --spec \"/**/*\"",
    "generate:images:debug": "cypress open --config screenshotsFolder=docs/assets/component-images/,integrationFolder=cypress/generate",
    "lint": "eslint --ext .vue,.ts --ignore-path .gitignore . --fix",
    "lint-watch": "esw --ext .vue,.ts --ignore-path .gitignore . --fix --watch",
    "postci": "codecov",
    "precommit": "yarn run test",
    "predev": "yarn run clean",
    "release:major": "npm run build:scss && changelog -M && git add CHANGELOG.md && git commit -m \"chore: update changelog\" && npm version major",
    "release:minor": "npm run build:scss && changelog -m && git add CHANGELOG.md && git commit -m \"chore: update changelog\" && npm version minor",
    "release:patch": "npm run build:scss && changelog -p && git add CHANGELOG.md && git commit -m \"chore: update changelog\" && npm version patch",
    "start": "nuxt-ts start",
    "test": "cross-env NODE_ENV=test concurrently \"yarn run lint\" \"yarn run unit\"",
    "unit": "cross-env NODE_ENV=test jest",
    "unit:update": "cross-env NODE_ENV=test jest --u",
    "unit:watch": "jest --watchAll --coverage=false"
  },
  "husky": {
    "hooks": {
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS",
      "pre-commit": "yarn run precommit"
    }
  },
  "resolutions": {
    "prosemirror-model": "1.9.1",
    "prosemirror-state": "1.3.3",
    "prosemirror-transform": "1.2.4",
    "prosemirror-view": "1.14.6"
  },
  "dependencies": {
    "bulma": "^0.8.2"
  },
  "devDependencies": {
    "@babel/core": "^7.9.6",
    "@babel/plugin-proposal-class-properties": "^7.8.3",
    "@babel/plugin-proposal-decorators": "^7.8.3",
    "@commitlint/cli": "^8.3.5",
    "@commitlint/config-conventional": "^8.3.4",
    "@fortawesome/fontawesome-free": "^5.13.0",
    "@nuxt/typescript-build": "^0.6.6",
    "@nuxt/typescript-runtime": "0.4.6",
    "@nuxtjs/dotenv": "^1.4.1",
    "@nuxtjs/eslint-config": "^3.0.0",
    "@nuxtjs/eslint-module": "^2.0.0",
    "@nuxtjs/google-analytics": "^2.2.3",
    "@nuxtjs/module-test-utils": "^1.6.1",
    "@nuxtjs/pwa": "^3.0.0-beta.20",
    "@nuxtjs/robots": "^2.4.2",
    "@nuxtjs/sitemap": "^2.3.0",
    "@nuxtjs/stylelint-module": "4.0.0",
    "@percy/cypress": "^2.3.1",
    "@popperjs/core": "^2.4.0",
    "@types/cypress": "^1.1.3",
    "@types/glob": "^7.1.1",
    "@types/jest": "^25.2.2",
    "@types/jquery": "^3.3.38",
    "@types/lodash": "^4.14.151",
    "@types/node": "14.0.1",
    "@types/popper.js": "^1.11.0",
    "@types/prettier": "^2.0.0",
    "@types/prosemirror-model": "^1.7.2",
    "@types/prosemirror-state": "^1.2.4",
    "@types/vue": "^2.0.0",
    "@types/file-saver": "^2.0.1",
    "@types/jszip": "^3.4.1",
    "@typescript-eslint/eslint-plugin": "^2.33.0",
    "@typescript-eslint/parser": "^2.33.0",
    "@vue/server-test-utils": "^1.0.2",
    "@vue/test-utils": "^1.0.2",
    "babel-core": "^7.0.0-bridge.0",
    "babel-eslint": "^10.1.0",
    "babel-jest": "26.0.1",
    "buefy": "^0.8.18",
    "clean-css-cli": "^4.3.0",
    "codecov": "^3.6.5",
    "concurrently": "5.2.0",
    "copy-to-clipboard": "^3.3.1",
    "core-js": "2.6.11",
    "cross-env": "^7.0.2",
    "cypress": "^4.6.0",
    "cypress-axe": "^0.8.1",
    "eslint": "^7.0.0",
    "eslint-config-prettier": "^6.11.0",
    "eslint-plugin-nuxt": "^1.0.0",
    "eslint-plugin-prettier": "^3.1.3",
    "eslint-watch": "^6.0.1",
    "file-saver": "^2.0.2",
    "generate-changelog": "^1.8.0",
    "glob": "7.1.6",
    "html-loader": "^1.1.0",
    "husky": "^4.2.5",
    "jest": "^26.0.1",
    "jest-serializer-vue": "^2.0.2",
    "jest-transform-stub": "^2.0.0",
    "jquery": "^3.5.1",
    "cookie-universal": "^2.1.4",
    "jszip": "^3.4.0",
    "jszip-utils": "^0.1.0",
    "lint-staged": "^10.2.2",
    "lodash": "^4.17.15",
    "node-sass": "^4.14.1",
    "node-sass-package-importer": "^5.3.2",
    "nuxt": "^2.12.2",
    "nuxt-property-decorator": "^2.7.2",
    "prettier": "^2.0.5",
    "prosemirror-model": "^1.9.1",
    "prosemirror-state": "^1.3.3",
    "raw-loader": "^4.0.1",
    "redirect-ssl": "^1.4.1",
    "require-context": "^1.1.0",
    "sass-loader": "^8.0.2",
    "stylelint": "^13.3.3",
    "stylelint-config-recommended": "^3.0.0",
    "stylelint-scss": "^3.17.2",
    "tiptap": "^1.27.1",
    "tiptap-extensions": "^1.29.1",
    "ts-jest": "^26.0.0",
    "ts-loader": "^7.0.4",
    "typescript": "^3.9.2",
    "vue": "^2.6.11",
    "vue-eslint-parser": "^7.1.0",
    "vue-highlightjs": "^1.3.3",
    "vue-jest": "^3.0.5",
    "vue-progressbar": "^0.7.5",
    "vue-property-decorator": "^8.4.2",
    "vue-template-compiler": "^2.6.11",
    "vuedraggable": "^2.23.2",
    "vuex-module-decorators": "^0.17.0"
  }
}
